{
  "name": "@modernpoacher/zashiki-govuk-frontend",
  "version": "1.0.23",
  "description": "GOV.UK Frontend for Zashiki",
  "main": "./index.js",
  "keywords": [
    "Nunjucks",
    "GOV.UK Frontend",
    "Shinkansen",
    "Transmission",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/zashiki-govuk-frontend.git"
  },
  "author": "Jonathan Perry for Modern Poacher Limited",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run lint:fix && npm run babel",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src test *.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm i shinkansen-transmission --no-save && NODE_ENV=test mocha test --recursive -r @babel/register -r @babel/polyfill --verbose",
    "pre-commit": "npm run lint && npm run test"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "mocha": "^7.1.2",
    "nunjucks": "^3.2.1",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "govuk-frontend": "^3.6.0",
    "shinkansen-rails": "1.0.7",
    "shinkansen-signals": "1.0.7",
    "shinkansen-transmission": "2.1.3",
    "@hapi/boom": "^9.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
