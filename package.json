{
  "name": "@modernpoacher/zashiki-govuk-frontend",
  "version": "1.0.31",
  "description": "GOV.UK Frontend for Zashiki",
  "main": "./index.js",
  "keywords": [
    "GOV.UK Frontend",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/zashiki-govuk-frontend.git"
  },
  "author": "Jonathan Perry for Modern Poacher Limited",
  "license": "UNLICENSED",
  "scripts": {
    "prepublishOnly": "npm run lint:fix && npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run babel",
    "build:clean": "npm run clean && cross-env NODE_ENV=production npm run build",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint *.js src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha test --recursive -r @babel/register -r @babel/polyfill --verbose",
    "pre-commit": "npm run lint && npm run test && npm run build && git add lib"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "shinkansen-rails": "1.0.11",
    "shinkansen-signals": "1.0.10",
    "shinkansen-transmission": "2.1.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "eslint": "^7.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "govuk-frontend": "^3.7.0",
    "husky": "^4.2.5",
    "mocha": "^8.0.1",
    "nunjucks": "^3.2.1",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "govuk-frontend": "^3.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
