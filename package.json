{
  "name": "@modernpoacher/zashiki-govuk-frontend",
  "version": "1.0.38",
  "description": "GOV.UK Frontend for Zashiki",
  "keywords": [
    "GOV.UK Frontend",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/zashiki-govuk-frontend.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha test --recursive -r @babel/register --verbose",
    "pre-commit": "npm run lint && npm run test && npm run build && git add lib"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "shinkansen-rails": "1.0.18",
    "shinkansen-signals": "1.1.2",
    "shinkansen-transmission": "2.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.3.7",
    "core-js": "^3.27.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "govuk-frontend": "^4.5.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nunjucks": "^3.2.3",
    "rimraf": "^4.1.2"
  }
}
