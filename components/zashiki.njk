{% macro zashiki(params) %}
  {% if params.meta.component %}

  {% elif params.meta.type === 'string' %}
    {% set field = params.field %}
{%- include "./string.njk" -%}

  {% elif params.meta.type === 'number' %}
    {% set field = params.field %}
{%- include "./number.njk" -%}

  {% elif params.meta.type === 'object' %}
{%- include "./object.njk" -%}

  {% elif params.meta.type === 'array' %}
{%- include "./array.njk" -%}

  {% elif params.meta.type === 'boolean' %}
    {% set field = params.field %}
{%- include "./boolean.njk" -%}

  {% elif params.meta.type === 'null' %}
    {% set field = params.field %}
{%- include "./null.njk" -%}

  {% endif %}
{% endmacro %}
